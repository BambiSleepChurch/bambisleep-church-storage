{
  "name": "brandyfication-mcp-server",
  "version": "1.1.0",
  "description": "BRANDYFICATION MCP Server with HTTP streaming support",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "brandyfication-mcp": "dist/index.js",
    "brandyfication-http": "dist/http-server.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:http": "node dist/http-server.js",
    "start:all": "concurrently \"npm run start\" \"npm run start:http\"",
    "dev": "tsc && node dist/index.js",
    "dev:http": "tsc && node dist/http-server.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "node-media-server": "^4.2.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "mcp",
    "file-hosting",
    "video-streaming",
    "brandyfication",
    "hls",
    "rtmp"
  ],
  "author": "BambiSleepChurch",
  "license": "MIT"
}
